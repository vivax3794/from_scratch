expose main() u8 {
    let mut x: u8 = 254;
    x += 1;
    assert x == 255;
    x += 1;
    assert x == 255;

    let mut x: 0..5 = 4;
    x += 1;
    assert x == 5;
    x += 1;
    assert x == 5;

    let mut x: 0..10 = 4;
    x *= 2;
    assert x == 8;
    x *= 2;
    assert x == 10;

    return 0;
}
