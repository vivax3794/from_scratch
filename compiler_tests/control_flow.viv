expose main() u8 {
    let mut x: u8 = 0;
    if true {
        x = 10;
    }
    assert x == 10;

    let mut x: u8 = 0;
    if false {
        x = 10;
    }
    assert x == 0;

    let mut x: u8 = 0;
    if false {
        x = 10;
    } else {
        x = 100;
    }
    assert x == 100;

    let mut x: u8 = 0;
    if false {
        x = 10;
    } else if true {
        x = 100;
    } else {
        x = 200;
    }
    assert x == 100;

    let mut x: u8 = 0;
    if false {
        x = 10;
    } else if false {
        x = 100;
    } else {
        x = 200;
    }
    assert x == 200;

    let x: u16 = 200;
    if x < 256 {
        let y: u8 = x;
    }

    return 0;
}
